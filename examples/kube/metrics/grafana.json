{
    "kind": "Pod",
    "apiVersion": "v1",
    "metadata": {
        "name": "crunchy-grafana",
        "labels": {
            "name": "crunchy-grafana"
        }
    },
    "spec": {
        "containers": [{
            "name": "crunchy-grafana",
            "image": "crunchydata/crunchy-grafana:$CCP_IMAGE_TAG",
            "ports": [{
                "containerPort": 3000,
                "protocol": "TCP"
            }],
            "env": [],
            "securityContext": {
                "capabilities": {}
            },
            "volumeMounts": [{
                "mountPath": "/data",
                "name": "data",
                "readOnly": false
            }]
        }],
        "volumes": [{
            "name": "data",
            "emptyDir": {}
        }]
    }
}
