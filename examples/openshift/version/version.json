{
    "kind": "Pod",
    "apiVersion": "v1",
    "metadata": {
        "name": "myversion",
        "labels": {
            "name": "myversion"
        }
    },
    "spec": {
        "containers": [{
            "name": "myversion",
            "image": "crunchydata/crunchy-version:1.2.1",
            "volumeMounts": [{
                "mountPath": "/gitrepo",
                "name": "gitrepo"
            }],
            "env": [{
                "name": "GIT_USER_NAME",
                "value": "jmccormick2001"
            }, {
                "name": "GIT_USER_EMAIL",
                "value": "djm29492@gmail.com"
            }, {
                "name": "PG_MASTER_SERVICE",
                "value": "single-master"
            }, {
                "name": "PG_MASTER_USER",
                "value": "master"
            }, {
                "name": "PG_MASTER_PASSWORD",
                "value": "password"
            }, {
                "name": "PG_MASTER_PORT",
                "value": "5432"
            }, {
                "name": "PG_DATABASE",
                "value": "postgres"
            }]
        }],
        "volumes": [{
            "name": "gitrepo",
            "gitRepo": {
                "repository": "https://f314a78ec16eec7c03f824a9a4b70bf52f5f01c1:x-oauth-basic@github.com/jmccormick2001/testproject"
            }
        }]
    }
}
